{
    "name": "inspiredminds/contao-content-templates",
    "description": "Contao extension that allows you to define content templates for your page content.",
    "type": "contao-bundle",
    "homepage": "https://github.com/inspiredminds/contao-content-templates",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "homepage": "http://www.inspiredminds.at",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/inspiredminds/contao-content-templates/issues",
        "source": "https://github.com/inspiredminds/contao-content-templates"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": ">=8.2",
        "contao/core-bundle": "^4.13 || ^5.3",
        "doctrine/dbal": "^2.11 || ^3.0 || ^4.0",
        "symfony/config": "^5.4 || ^6.4 || ^7.3",
        "symfony/dependency-injection": "^5.4 || ^6.4 || ^7.3",
        "symfony/http-foundation": "^5.4 || ^6.4 || ^7.3",
        "symfony/http-kernel": "^5.4 || ^6.4 || ^7.3",
        "symfony/polyfill-php80": "^1.26",
        "symfony/routing": "^5.4 || ^6.4 || ^7.3",
        "symfony/security-core": "^5.4 || ^6.4 || ^7.3",
        "symfony/translation-contracts": "^1.0 || ^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoContentTemplates\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoContentTemplates\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^6.0",
        "contao/manager-plugin": "^2.12",
        "contao/rector": "^1.0",
        "maglnet/composer-require-checker": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "contao/manager-plugin": true
        }
    }
}
